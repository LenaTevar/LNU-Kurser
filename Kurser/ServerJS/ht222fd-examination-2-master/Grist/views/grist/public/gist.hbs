<div class="card">
    <div class="card-body">
        <h5 class="card-title">{{data.title}}</h5>
        <h6 class="card-subtitle mb-2 text-muted">By: {{data.author}}</h6>
        <p class="card-text code">{{data.content}}</p>
        <h6 class="card-subtitle mb-2 text-muted">Tags: {{data.tags}}</h6>
    </div>
</div>